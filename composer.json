{
    "name": "ney/laravel-package-bone",
    "description": "Starter project for developing Laravel package.",
    "keywords": ["laravel", "package", "starter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ney Chang",
            "email": "zoobile@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
      "psr-4": {
        "Ney\\PackageBone\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Ney\\PackageBone\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
