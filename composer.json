{
    "name": "ney/laravel-package-bone",
    "description": "Starter project for developing Laravel package.",
    "keywords": ["laravel", "package", "starter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ney Chang",
            "email": "zoobile@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
      "behat/behat": "^3.3",
      "behat/mink": "^1.7",
      "behat/mink-extension": "^2.2",
      "fzaninotto/faker": "~1.4",
      "laracasts/behat-laravel-extension": "^1.0",
      "mockery/mockery": "0.9.*",
      "phpspec/phpspec": "^3.2",
      "phpunit/phpunit": "~5.7"
    },
    "autoload": {
      "psr-4": {
        "Ney\\PackageBone": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Ney\\PackageBone\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
